<!-- Google Analytics -->

<% if (config.ga_track_id) { %>
<!-- Google tag (gtag.js) -->
<script
  async
  src="https://www.googletagmanager.com/gtag/js?id=<%= config.ga_track_id %>"
></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag() {
    dataLayer.push(arguments);
  }
  gtag("js", new Date());

  gtag("config", "<%= config.ga_track_id %>");
</script>

<% } %>
